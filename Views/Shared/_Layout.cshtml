<!DOCTYPE html>
<!--[if IE 8 ]>    <html class="no-js ie8" lang="en"> <![endif]-->
<!--[if IE 9 ]>    <html class="no-js ie9" lang="en"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html class="no-js" lang="en">
<!--<![endif]-->
<head lang="en">
    <meta charset="UTF-8">
    <meta name="description" content="Jan Anderson's blog site">
    <meta name="author" content="CreateIT">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, maximum-scale=1, shrink-to-fit=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="keywords" content="web development, app development, full stack coding, f">

    <title>@ViewBag.Title - Jan's blog</title>

    <link rel="icon" href="~/Template/assets/images/content/tailor/favicon.ico">

    <link rel="stylesheet" type="text/css" href="~/Template/assets/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Template/assets/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/Template/assets/css/computerservice.css">

    <!--[if lt IE 9]>
    <script src="./assets/bootstrap/js/html5shiv.min.js"></script>
    <script src="./assets/bootstrap/js/respond.min.js"></script>
    <![endif]-->

    <script src="~/Template/assets/js/modernizr.custom.js"></script>
</head>
<header class="ct-mediaSection ct-section" id="header" data-stellar-background-ratio="0.3" data-height="200" data-type="parallax" data-bg-image="~/Template/assets/images/content/computerservice/computer-home-main-bg.jpg" data-bg-image-mobile="~/Template/assets/images/content/computerservice/computer-home-main-bg.jpg">
</header>
<body class="cssAnimate">
    <div class="ct-preloader">
        <div class="ct-preloader-content">
        </div>
    </div>
    <div class="main">
        <div class="ct-topbar ct-topbar--default">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6 col-lg-6">
                        <div class="text-wrapper">
                            <i class="fa fa-phone"></i>
                            828-782-xxxx
                        </div>
                        <div class="text-wrapper">
                            <a href="#"><i class="fa fa-envelope"></i>DirectstepDevelopment@gmail.com</a>
                        </div>
                    </div>
                    <div class="col-sm-6 col-lg-6">
                        <div class="ct-socials text-right">
                            <a href="https://www.facebook.com/createITpl" data-toggle="tooltip" data-placement="bottom" title="Facebook"><i class="fa fa-facebook"></i></a>
                            <a href="https://twitter.com/createitpl" data-toggle="tooltip" data-placement="bottom" title="Twitter"><i class="fa fa-twitter"></i></a>
                            <a href="#" data-toggle="tooltip" data-placement="bottom" title="Google+"><i class="fa fa-google-plus"></i></a>
                            <a href="#" data-toggle="tooltip" data-placement="bottom" title="LinkedIn"><i class="fa fa-linkedin"></i></a>
                            <a href="#" data-toggle="tooltip" data-placement="bottom" title="Pinterest"><i class="fa fa-pinterest-square"></i></a>
                            <a href="#" data-toggle="tooltip" data-placement="bottom" title="Dribble"><i class="fa fa-dribbble"></i></a>
                            <a href="#" data-toggle="tooltip" data-placement="bottom" title="Tumblr"><i class="fa fa-tumblr"></i></a>
                            <a href="#" data-toggle="tooltip" data-placement="bottom" title="Instagram"><i class="fa fa-instagram"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="ct-navbarMobile">
            <div class="navbar-header">
                <a class="navbar-brand" href="indexmulti.html">
                    <img class="ct-logo-navbar--transparent" src="~/Template/assets/images/content/computerservice/logo.png" alt="">
                    <img class="ct-logo-navbar--solid" src="~/Template/assets/images/content/computerservice/logo-solid.png" alt="">
                </a>
            </div>
            <button type="button" class="navbar-toggle">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
        </div>
        <div class="ct-menuMobile">
            <ul class="ct-menuMobile-navbar">
                <li>@Html.ActionLink("Home", "Index", "BlogPosts")</li>
                @*<li>@Html.ActionLink("About", "About", "Home")</li>*@
                <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
            </ul>
            @Html.Partial("_LoginPartial")
        </div>
        <div id="ct-js-wrapper" class="ct-pageWrapper">
            <nav class="navbar ct-navbar--transparent ct-navbar--stickyToTop ct-navbar--withTopbar" role="navigation">
                <div class="container">
                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-right">
                            <li>@Html.ActionLink("Home", "Index", "BlogPosts")</li>
                            @*<li>@Html.ActionLink("About", "About", "Home")</li>*@
                            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                        </ul>
                        @Html.Partial("_LoginPartial")
                    </div>
                </div>
            </nav>

            @RenderBody()
            <hr />
            <footer>
                <div class="container">
                    <div class="row">
                        <div class="col-sm-5 totop "><a id="ct-js-totop" href="#header">Go to the top <i class="fa fa-angle-up"></i></a> </div>
                        <div class="col-sm-2 ct-footer-logo"><a href="indexmulti.html"><img src="./assets/images/content/computerservice/logo-footer.png" alt=""></a></div>
                        <div class="col-sm-5 text-right copyright">© 2014 <a href="http://themeplayers.net/">Theme Players</a></div>
                    </div>
                </div>
            </footer>
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)


    <script src="~/Template/assets/js/jquery.min.js"></script>
    <script src="~/Template/assets/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Template/assets/js/helpers.js"></script>

    <script src="~/Template/assets/js/device.min.js"></script>
    <script src="~/Template/assets/js/jquery.browser.min.js"></script>
    <script src="~/Template/assets/js/snap.min.js"></script>
    <script src="~/Template/assets/js/jquery.appear.js"></script>

    <script src="~/Template/assets/js/smoothscroll.js"></script>

    <script src="~/Template/assets/js/select2/select2.min.js"></script>
    <script src="~/Template/assets/js/select2/init.js"></script>

    <script src="~/Template/assets/js/ct-mediaSection/jquery.stellar.min.js"></script>
    <script src="~/Template/assets/js/ct-mediaSection/init.js"></script>

    <script src="~/Template/assets/js/slick/slick.min.js"></script>
    <script src="~/Template/assets/js/slick/init.js"></script>

    <script src="~/Template/assets/js/main.js"></script>


</body>
</html>
